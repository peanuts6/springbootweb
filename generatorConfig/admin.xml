<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry
		location="/Users/xqy/.m2/repository/mysql/mysql-connector-java/6.0.5/mysql-connector-java-6.0.5.jar" />

	<context id="context1" targetRuntime="MyBatis3">
		<plugin type="com.softwareloop.mybatis.generator.plugins.LombokPlugin">
             <property name="builder" value="true"/>
             <property name="allArgsConstructor" value="true"/>
        </plugin>
        
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
			<property name="addRemarkComments" value="false" />
			<property name="javaFileEncoding" value="UTF-8" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost/imking" userId="root"
			password="123456">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<javaModelGenerator targetPackage="xqy.msa.springbootweb.admin.domain" targetProject="MAVEN">
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="xqy.msa.springbootweb.admin.xml"  targetProject="MAVEN">
    	</sqlMapGenerator>
		<javaClientGenerator targetPackage="xqy.msa.springbootweb.admin.domain"
			targetProject="MAVEN" type="XMLMAPPER">
		</javaClientGenerator>
		
		<table schema="imking" tableName="imk_users" domainObjectName="User"/>
		<table schema="imking" tableName="imk_roles" domainObjectName="Role"/>
		
	</context>
</generatorConfiguration>